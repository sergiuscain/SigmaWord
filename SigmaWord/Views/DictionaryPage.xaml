<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SigmaWord.Views.DictionaryPage"
             xmlns:viewmodel="clr-namespace:SigmaWord.ViewModels"
             xmlns:models="clr-namespace:SigmaWord.Data.Entities"
             x:DataType="viewmodel:DictionaryViewModel">
    <VerticalStackLayout Style="{StaticResource DarkBackgroundStyle}">
        <Button Style="{StaticResource PrimaryButtonStyle}" 
            Text="Oxford3000 A1"
            Command="{Binding GoToWordsPageCommand}"
            CommandParameter="Oxford3000_a1Ready"/>
        <Button Style="{StaticResource PrimaryButtonStyle}" 
            Text="Oxford3000 A2"
            Command="{Binding GoToWordsPageCommand}"
            CommandParameter="Oxford3000_a2Ready"/>
        <Button Style="{StaticResource PrimaryButtonStyle}" 
            Text="Oxford3000 B1"
            Command="{Binding GoToWordsPageCommand}"
            CommandParameter="Oxford3000_b1Ready"/>
        <Button Style="{StaticResource PrimaryButtonStyle}" 
            Text="Oxford3000 B2"
            Command="{Binding GoToWordsPageCommand}"
            CommandParameter="Oxford3000_b2Ready"/>
    </VerticalStackLayout>
</ContentPage>
